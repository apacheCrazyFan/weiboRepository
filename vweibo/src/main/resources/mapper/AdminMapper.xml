<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.weibo.mapper.AdminMapper">
	<select id="login" parameterType="Admin" resultType="Admin">
		select * from Admin where Aname = #{Aname,jdbcType=VARCHAR} and Apwd = #{Apwd,jdbcType=VARCHAR}
	</select>
	
</mapper>